av *READ-WRITE* uns8 @bp =@ local[81]
av uns8 @cp = "Hello!"
int i = 0
while i < @cp.upper:
    bp[i] = cp[i]
    next i = i + 1
bp[cp.upper] = 0

int X = 5
ap *READ-WRITE* int @Y    // `=@ local' is implied
Y = X + 2                 // Now Y == 7
Y = Y - 4                 // Now Y == 3
X = X + 1                 // Illegal!  X is co
ap ro int @Z = @Y         // Copies pointer value @Y.
                          // Variable conversion from *READ-WRITE*
                          // to ro is legal.

